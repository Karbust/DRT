<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/holder_notificacao"
        android:layout_width="match_parent"
        android:layout_height="167dp"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/viagens_branco" />

    <TextView
        android:id="@+id/Data_notificacao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="30dp"
        android:text="Data"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintStart_toStartOf="@+id/holder_notificacao"
        app:layout_constraintTop_toTopOf="@+id/holder_notificacao" />

    <TextView
        android:id="@+id/notificacao"
        android:layout_width="300dip"
        android:layout_height="wrap_content"
        android:singleLine="false"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintStart_toStartOf="@+id/Data_notificacao"
        app:layout_constraintTop_toBottomOf="@+id/Data_notificacao" />

</androidx.constraintlayout.widget.ConstraintLayout>